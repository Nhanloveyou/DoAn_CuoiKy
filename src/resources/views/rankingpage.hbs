<div class="ranking">
<h1 class="hq">Bảng Xếp hạng</h1>
<img class="play" src="img/ranking.svg" alt="">
{{#each users}}
<div class="container">
  <div class="mt-4 mb-4">
    <table class="mid">
      <tr>
          <td rowspan="2" class="rank">{{sum @index 1}}</td>
          <td class="list-group-item list-group-item-success frame">Name:  {{this.name}}</td>
      </tr>
      <tr>
          <td class="list-group-item frame">Exp:  {{this.exp}}</td>
      </tr>
    </table>
  </div>
</div>
{{/each}}
</div>